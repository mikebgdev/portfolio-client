{
    "name": "mikebgdev/portfolio-client",
    "description": "Portfolio Client",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "ext-json": "*"
    },
    "require-dev": {
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
